   "source": [
    "data = input()\n",
    " \n",
    "a = data.split(' ')\n",
    "result = ''\n",
    "\n",
    "for i in range(len(a)):\n",
    "    result += a[len(a)-1-i]\n",
    "    result += \" \"\n",
    "print()\n",
    "print(\"output:\")\n",
    "print(result)"
   ]
